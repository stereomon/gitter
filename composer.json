{
    "name": "gitter/gitter",
    "type": "library",
    "description": "Helps with git repositories.",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.1",
        "symfony/options-resolver": "^5.0",
        "symfony/process": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.12.18",
        "slevomat/coding-standard": "^6.2",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Gitter\\": "src/Gitter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GitterTest\\": "tests/",
            "GitterTest\\Helper\\": "tests/_support/Helper/"
        }
    },
    "bin": [
        "bin/gitter"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
